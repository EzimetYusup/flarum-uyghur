{
  "name": "sayiwen/flarum-uyghur",
  "type": "flarum-extension",
  "description": "Arabic Language Pack.",
  "keywords": ["locale", "Arabic", "Language"],
  "license": "MIT",
  "require": {
      "flarum/core": "^0.1.0-beta.8"
  },
  "autoload": {
    "psr-4": {
      "Sayiwen\\FlarumUyghur\\": "src/"
    }
  },
  "extra": {
      "flarum-extension": {
          "title": "Uyghur Language",
          "icon": {
              "image": "icon.svg",
              "backgroundColor": "#006233",
              "backgroundSize": "cover",
              "backgroundPosition": "center"
          }
      },
      "flarum-locale": {
          "code": "ug",
          "title": "Uyghur"
      }
  }
}
